{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "minimumReleaseAge": "14 days",
  "extends": [
    "github>elastic/renovate-config"
  ],
  "labels": [
    "dependencies",
    "v7.17.7",
    "v8.6.4"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    }
  ]
}
